{
    "require-dev": {
        "phpunit/phpunit": "*",
        "mockery/mockery": "*",
        "symfony/var-dumper": "*"
    },
    "autoload": {
        "psr-4": {
            "Acme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acme\\Test\\": "tests/Acme/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload --optimize"
        ],
        "post-update-cmd": [
            "composer dump-autoload --optimize"
        ]
    }
}
